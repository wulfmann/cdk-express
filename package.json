{
  "name": "cdk-express",
  "version": "0.0.4",
  "description": "Express-like framework for building api-gateway and lambda applications with CDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:wulfmann/cdk-express.git",
  "author": "Joe Snell <joepsnell@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@aws-cdk/assert": "1.42.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.9",
    "aws-cdk": "1.42.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "test": "jest",
    "lint": "tslint '{src/**/*,test/**/*}.ts'",
    "format": "prettier '{src/**/*,test/**/*}.ts'"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.42.1",
    "@aws-cdk/aws-lambda": "^1.42.1",
    "@aws-cdk/core": "1.42.1",
    "path-to-regexp": "^6.1.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/lib/"
    ]
  }
}
